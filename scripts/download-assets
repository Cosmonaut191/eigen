#!/usr/bin/env yarn zx


if (process.env === true) {
  await   $`./scripts/setup-distribute`
}

await $`aws s3 cp s3://artsy-citadel/eigen/google-services.json android/app/google-services.json`
await $`aws s3 cp s3://artsy-citadel/eigen/GoogleService-Info.plist GoogleService-Info.plist`

await $`./scripts/download-fonts`
